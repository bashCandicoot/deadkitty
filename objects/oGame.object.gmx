            <string>// Quitting \\
if keyboard_check_pressed(vk_escape) {
    game_end() }
    
// Restart \\
if keyboard_check_pressed(ord("R")) {
    audio_stop_sound(aMusicCarelessChoices)
    room_restart() }
        
secondsSinceGameStarted += 1/room_speed
secondsSinceDogSpawn += 1/room_speed
secondsSinceBirdSpawn += 1/room_speed

if (numberOfDogs &lt; 2) {
    spawnDog(1.90, false, false) }
 
else if (numberOfDogs &gt;= 2 &amp;&amp; numberOfDogs &lt; 8) {
    spawnDog(2.0, false, false) }

else if (numberOfDogs == 9) { 
    spawnDog(1.90, false, false) }      
        
if (oUnitColumnScore.score == 8) {
    oBackground0.image_alpha -= 0.04
    oBackground1.image_alpha += 0.04
}        

if (oUnitColumnScore.score == 16) {
    oBackground1.image_alpha -= 0.02
    oBackground2.image_alpha += 0.02
}

if (numberOfBirds &lt; 2) {
    if (oUnitColumnScore.score &gt;= 8) {
        if (secondsSinceBirdSpawn &gt; 1.6) {
            instance_create(-50, 1280, oBird)
            secondsSinceBirdSpawn = 0
            numberOfBirds++ } } }

if (numberOfBirds &gt;= 2 &amp;&amp; numberOfBirds &lt; 4) {
        if (secondsSinceBirdSpawn &gt; 2) {
            instance_create(-50, 1280, oBird)
            secondsSinceBirdSpawn = 0
            numberOfBirds++ } }             

if (numberOfBirds &gt;= 4 &amp;&amp; numberOfBirds &lt; 12) {
    bird = instance_create(-50, 1280, oBird)
    if (numberOfBirds == 4) {
        with (bird) {
            image_alpha = oGame.birdOpacity }
            numberOfBirds++ } 
    else {
        with (bird) {
           oGame.birdOpacity = .5
           counted = true 
           image_alpha = oGame.birdOpacity }
           numberOfBirds++ } }
        
if (numberOfBirds &gt;= 12 &amp;&amp; numberOfBirds &lt; 16) {
        if (secondsSinceBirdSpawn &gt; 2) {
            instance_create(-50, 1280, oBird)
            secondsSinceBirdSpawn = 0
            numberOfBirds++ } } 
            
if (numberOfBirds &gt;= 16 &amp;&amp; numberOfBirds &lt; 24) {
    bird = instance_create(-50, 1280, oBird)
    if (numberOfBirds == 15) {
        with (bird) {
            image_alpha = oGame.birdOpacity }
            numberOfBirds++ } 
    else {
        with (bird) {
           oGame.birdOpacity = .5
           counted = true 
           image_alpha = oGame.birdOpacity }
           numberOfBirds++ } }
           
if (secondsSinceGameStarted &gt;= 35) {
    spawnDog(2.0, false, true)
    spawnDog(2.0, true, false) }
           
if (keyboard_check(ord('M'))) {
    show_message(secondsSinceDogSpawn) }

</string>
